<patTemplate:tmpl name="page">
<div>
	Create an HTML image:<br>
	<patTemplate:var name="image_src" modifier="HTML_Img" title="This image has autogenerated width and height attributes"/>
</div>
<br />
<div>
	Create one from a default value<br>
	<patTemplate:var name="google" default="http://www.google.de/intl/de_de/images/logo.gif" modifier="HTML_Img" title="This is generated from a default value."/>
</div>
</patTemplate:tmpl>
